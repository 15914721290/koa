{
  "name": "51hjgt",
  "version": "2.0.0",
  "scripts": {
    "start": "npm-run-all -p watch-dev start-dev",
    "start-dev": "set NODE_ENV=local && nodemon --inspect index.js",
    "watch-dev": "webpack --config ./config/webpack.config.dev.js -w",
    "build": "webpack --config ./config/webpack.config.prod.js",
    "pm2": "pm2 start ./config/pm2.config.js",
    "pm2-prod": "pm2 start ./config/pm2.prod.config.js",
    "reload": "pm2 reload ./config/pm2.config.js",
    "delete": "pm2 delete ./config/pm2.config.js",
    "clean": "git clean -fX app static",
    "lint": "eslint ./app/**/*.js --ignore-path .eslintignore"
  },
  "dependencies": {
    "js-md5": "^0.7.3",
    "koa": "^2.5.2",
    "koa-bodyparser": "^4.2.1",
    "koa-compress": "^3.0.0",
    "koa-nunjucks-2": "^3.0.2",
    "koa-router": "^7.4.0",
    "moment": "^2.22.2",
    "node-fetch": "^2.1.2",
    "nunjucks": "^3.1.3",
    "pm2": "^3.0.2"
  },
  "devDependencies": {
    "autoprefixer": "^9.0.0",
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "chokidar": "^2.0.4",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "cssnano": "^4.0.3",
    "eslint": "^4.19.1",
    "eslint-plugin-import": "^2.13.0",
    "extract-loader": "^2.0.1",
    "file-loader": "^1.1.11",
    "koa-static": "^5.0.0",
    "less": "^3.7.1",
    "less-loader": "^4.1.0",
    "nodemon": "^1.18.3",
    "npm-run-all": "^4.1.3",
    "postcss-loader": "^2.1.6",
    "postcss-px2rem": "^0.3.0",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^4.16.1",
    "webpack-cli": "^3.1.0"
  }
